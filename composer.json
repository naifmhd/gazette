{
    "name": "naifmhd/gazette",
    "description": "A package to fetch Jobs and Tenders from Gazette.gov.mv",
    "license": "MIT",
    "authors": [
        {
            "name": "Naif Mohamed",
            "email": "naifmhd@gmail.com",
            "homepage": "https://naif.mv"
        }
    ],
    "homepage": "https://github.com/naifmhd/gazette",
    "keywords": ["Laravel", "Gazette"],
    "require": {
        "illuminate/support": "~7|~8"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6"
    },
    "autoload": {
        "psr-4": {
            "Naifmhd\\Gazette\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Naifmhd\\Gazette\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Naifmhd\\Gazette\\GazetteServiceProvider"
            ],
            "aliases": {
                "Gazette": "Naifmhd\\Gazette\\Facades\\Gazette"
            }
        }
    }
}
